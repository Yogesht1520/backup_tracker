

class Main {
    public static void main(String[] args) {
        //Using temp variable
        int a = 10, b = 20, temp;
        
        temp = a;
        a = b;
        b = temp;
        
        System.out.println(a);
        System.out.println(b);
        
        // Using 2 variables
        int c =77, d = 54;
        c = c+d;
        d = c-d;
        c = c-d;
        
        System.out.println("c="+c);
        System.out.println("d="+d);
    }
}